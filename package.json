{
  "name": "Dockman",
  "private": true,
  "version": "0.0.0",
  "main": "main.js",
  "scripts": {
    "dev-front": "watchify -dv -t vueify -e front/index.js -o build/front.js",
    "run": "electron .",
    "lint": "eslint ./main.js front/*.js",
    "postinstall": "mkdir build || true",
    "start": "npm run lint && concurrently \"npm run run -s\" \"npm run dev-front -s\""
  },
  "dependencies": {
    "browserify": "^14.4.0",
    "electron": "^1.6.10",
    "font-awesome": "^4.7.0",
    "material-design-lite": "^1.3.0",
    "vue": "^2.3.3",
    "vue-router": "^2.5.3",
    "vueify": "^9.4.1",
    "vuex": "^2.3.1"
  },
  "devDependencies": {
    "concurrently": "^3.4.0",
    "eslint": "^4.0.0",
    "watchify": "^3.9.0"
  }
}
